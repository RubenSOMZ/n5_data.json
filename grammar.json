    { "id": "20", "category": "Verbo", "title": "～てもいい", "formula": "V-te + もいい", "description": "Permiso para hacer algo.", "examples": [{ "jp": "行[い]ってもいいですか。", "es": "¿Puedo ir?" }] },
    { "id": "21", "category": "Verbo", "title": "～てはいけない", "formula": "V-te + はいけません", "description": "Prohibición.", "examples": [{ "jp": "入[はい]ってはいけません。", "es": "No se puede entrar." }] },
    { "id": "22", "category": "Adjetivo", "title": "ADJ-い (Pres)", "formula": "ADJ-い + です", "description": "Adjetivo i presente.", "examples": [{ "jp": "高[たか]いです。", "es": "Es caro." }] },
    { "id": "23", "category": "Adjetivo", "title": "ADJ-い (Pas)", "formula": "ADJ-i (-i) + かったです", "description": "Adjetivo i pasado.", "examples": [{ "jp": "良[よ]かったです。", "es": "Estuvo bien." }] },
    { "id": "24", "category": "Adjetivo", "title": "ADJ-い (Neg)", "formula": "ADJ-i (-i) + くないです", "description": "Adjetivo i negativo.", "examples": [{ "jp": "美味[おい]しくないです。", "es": "No está rico." }] },
