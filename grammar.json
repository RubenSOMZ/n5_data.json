    { "id": "17", "category": "Verbo", "title": "～ている", "formula": "V-te + いる", "description": "Estar haciendo (progresivo).", "examples": [{ "jp": "読[よ]んでいます。", "es": "Estoy leyendo." }] },
    { "id": "18", "category": "Verbo", "title": "～てください", "formula": "V-te + ください", "description": "Petición o instrucción.", "examples": [{ "jp": "待[ま]ってください。", "es": "Espere, por favor." }] },
