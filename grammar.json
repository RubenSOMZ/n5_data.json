{
  "grammar": [
    { "id": "1", "category": "Partícula", "title": "は (wa)", "formula": "S + は", "description": "Marcador de tema. Indica de qué estamos hablando.", "examples": [{ "jp": "私[わたし]は 学生[がくせい]です。", "es": "Yo soy estudiante." }] },
    { "id": "2", "category": "Partícula", "title": "も (mo)", "formula": "S + も", "description": "Significa 'también'. Sustituye a は o が.", "examples": [{ "jp": "私[わたし]も 行[い]きます。", "es": "Yo también iré." }] },
    { "id": "3", "category": "Partícula", "title": "が (ga)", "formula": "S + が", "description": "Marcador de sujeto. Destaca quién realiza la acción.", "examples": [{ "jp": "猫[ねこ]が います。", "es": "Hay un gato." }] },
    { "id": "4", "category": "Partícula", "title": "を (wo)", "formula": "S + を + O", "description": "Marcador de objeto directo. Indica qué recibe la acción.", "examples": [{ "jp": "水[みず]を 飲[の]みます。", "es": "Bebo agua." }] },
    { "id": "5", "category": "Partícula", "title": "に (ni)", "formula": "Lugar/Tiempo + に", "description": "Indica destino, existencia o tiempo específico.", "examples": [{ "jp": "学校[がっこう]に 行[い]きます。", "es": "Voy a la escuela." }] },
    { "id": "6", "category": "Partícula", "title": "で (de)", "formula": "Lugar + で", "description": "Lugar donde ocurre una acción o medio usado.", "examples": [{ "jp": "家[いえ]で 食[た]べます。", "es": "Como en casa." }] },
    { "id": "7", "category": "Partícula", "title": "の (no)", "formula": "S1 + の + S2", "description": "Indica posesión o relación.", "examples": [{ "jp": "私[わたし]の 本[ほん]です。", "es": "Es mi libro." }] },
    { "id": "8", "category": "Partícula", "title": "へ (he)", "formula": "Lugar + へ", "description": "Indica dirección de movimiento.", "examples": [{ "jp": "日本[にほん]へ 行[い]きたいです。", "es": "Quiero ir a Japón." }] },
    { "id": "9", "category": "Partícula", "title": "と (to)", "formula": "S + と + S", "description": "Significa 'y' o 'con' (compañía).", "examples": [{ "jp": "友達[ともだち]と 会[あ]います。", "es": "Me veo con un amigo." }] },
    { "id": "10", "category": "Partícula", "title": "から (kara)", "formula": "S + から", "description": "Significa 'desde' o 'porque'.", "examples": [{ "jp": "三時[さんじ]から です。", "es": "Es desde las tres." }] },
    { "id": "11", "category": "Partícula", "title": "まで (made)", "formula": "S + まで", "description": "Significa 'hasta'.", "examples": [{ "jp": "駅[えき]まで 歩[ある]きます。", "es": "Camino hasta la estación." }] },
    { "id": "12", "category": "Verbo", "title": "です (desu)", "formula": "S + です", "description": "Verbo ser/estar cortés.", "examples": [{ "jp": "これ[これ]は 本[ほん]です。", "es": "Esto es un libro." }] },
    { "id": "13", "category": "Verbo", "title": "～ます (masu)", "formula": "Raíz de V + ます", "description": "Presente/Futuro cortés.", "examples": [{ "jp": "食[た]べます。", "es": "Comeré." }] },
    { "id": "14", "category": "Verbo", "title": "～ました (mashita)", "formula": "Raíz de V + ました", "description": "Pasado cortés.", "examples": [{ "jp": "見[み]ました。", "es": "Lo vi." }] },
    { "id": "15", "category": "Verbo", "title": "～ません (masen)", "formula": "Raíz de V + ません", "description": "Negativo presente cortés.", "examples": [{ "jp": "行[い]きません。", "es": "No iré." }] },
