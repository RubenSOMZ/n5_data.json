    { "id": "26", "category": "Adjetivo", "title": "ADJ-な (Pas)", "formula": "ADJ-na + でした", "description": "Adjetivo na pasado.", "examples": [{ "jp": "有名[ゆうめい]でした。", "es": "Era famoso." }] },
    { "id": "27", "category": "Adjetivo", "title": "ADJ-na (Neg)", "formula": "ADJ-na + じゃありません", "description": "Adjetivo na negativo.", "examples": [{ "jp": "親切[しんせつ]じゃありません。", "es": "No es amable." }] },
    { "id": "28", "category": "Verbo", "title": "あります", "formula": "S + があります", "description": "Existencia (objetos).", "examples": [{ "jp": "車[くるま]が あります。", "es": "Hay un coche." }] },
    { "id": "29", "category": "Verbo", "title": "います", "formula": "S + がいます", "description": "Existencia (seres vivos).", "examples": [{ "jp": "犬[いぬ]が います。", "es": "Hay un perro." }] },
    { "id": "30", "category": "Expresión", "title": "～が好き", "formula": "S + が好きです", "description": "Gustar algo.", "examples": [{ "jp": "日本[にほん]が 好きです。", "es": "Me gusta Japón." }] },
    { "id": "31", "category": "Expresión", "title": "～が上手", "formula": "S + が上手です", "description": "Ser bueno en algo.", "examples": [{ "jp": "料理[りょうり]が 上手です。", "es": "Cocina bien." }] },
    { "id": "32", "category": "Expresión", "title": "～ができる", "formula": "V-dic + ことが できます", "description": "Poder hacer algo.", "examples": [{ "jp": "話[はな]すことが できます。", "es": "Puedo hablar." }] },
    { "id": "33", "category": "Pronombre", "title": "これ/それ/あれ", "formula": "Demostrativo", "description": "Esto, eso, aquello.", "examples": [{ "jp": "これは何ですか。", "es": "¿Qué es esto?" }] },
